<odoo>
   <record id="com_view_dev_config_form" model="ir.ui.view">
        <field name="name">com.view.dev.config.form</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="50"/>
         <field name="inherit_id" ref="base_setup.res_config_settings_view_form" />
        <field name="arch" type="xml">
        
        <xpath expr="//div[@data-key='general_settings']" position="inside">
	            <h2>Company Document Expiry Before Days </h2>
                    <div class="row mt16 o_settings_container">
                        <div class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="com_expiry_before_days"/>
                                <field name="com_expiry_before_days"/>
                            </div>
                        </div>
                    </div>     
	        </xpath>
		</field>
	</record>
	


</odoo>
